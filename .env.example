# Dremio Cloud Configuration
# Copy this file to .env and fill in your actual values

# Dremio Cloud URL (e.g., https://your-org.dremio.cloud)
DREMIO_CLOUD_URL=https://your-org.dremio.cloud

# Dremio Cloud credentials
DREMIO_USERNAME=your-username@example.com
DREMIO_PASSWORD=your-password

# Dremio Project ID (found in your Dremio Cloud project settings)
DREMIO_PROJECT_ID=your-project-id

# Flask Configuration
FLASK_DEBUG=true
FLASK_HOST=0.0.0.0
FLASK_PORT=5000
SECRET_KEY=your-secret-key-change-in-production

# Optional: Additional configuration
# FLASK_ENV=development
